package spring_practice.di.ui;

import spring_practice.di.entity.Exam;

public class GridExamConsole implements ExamConsoel {

	private Exam exam;
	
	
	 public GridExamConsole() {
		// TODO Auto-generated constructor stub
	}
	
	@Override
	public void print() {
		System.out.println("--------------------");
		System.out.println("  total      avg");
		System.out.println("--------------------");
		 System.out.printf("    %d      %5.2f", exam.total(), exam.avg());
		System.out.println("--------------------");
	}

}
